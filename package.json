{
  "name": "",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node server",
    "build": "webpack && npm run compile-sass && npm run move-src && npm run move-rest",
    "compile-sass": "npm run main-sass && npm run app-sass",
    "main-sass": "sass --style=compressed --sourcemap=none --no-cache  src/scss/main.scss build/assets/main.css",
    "app-sass": "sass --style=compressed --sourcemap=none --no-cache  src/scss/app.scss build/assets/app.css",
    "move-src": "cp -rf src/models/ build/models/ && cp -rf src/routes/ build/routes/ && cp -rf src/views/ build/views/",
    "move-rest": "cp -rf config/ build/config/ && cp -rf vendor/ build/assets/ && cp -f server.js package.json build/"
  },
  "dependencies": {
    "bcrypt-nodejs": "latest",
    "body-parser": "~1.0.0",
    "connect-ensure-login": "^0.1.1",
    "connect-flash": "~0.1.1",
    "cookie-parser": "~1.0.0",
    "express": "^4.14.0",
    "express-session": "~1.0.0",
    "immutable": "^3.8.1",
    "method-override": "~1.0.0",
    "mongodb": "~2.2.21",
    "mongoose": "~4.4",
    "morgan": "~1.0.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.1",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "webpack": "^1.14.0"
  },
  "author": "Russell Clarey",
  "license": "ISC"
}
